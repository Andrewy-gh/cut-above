{
  "name": "cut-above-sql",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development tsx watch src/index.ts",
    "build": "tsc",
    "start": "NODE_ENV=production node build/index.js",
    "worker:email:dev": "NODE_ENV=development tsx watch src/workers/emailOutboxWorker.ts",
    "worker:email": "NODE_ENV=production node build/workers/emailOutboxWorker.js",
    "test": "DOTENV_CONFIG_PATH=.env.test NODE_ENV=test vitest",
    "test:run": "DOTENV_CONFIG_PATH=.env.test NODE_ENV=test vitest run",
    "test:ui": "vitest --ui",
    "test:local": "DOTENV_CONFIG_PATH=.env.test NODE_ENV=test vitest",
    "test:local:run": "DOTENV_CONFIG_PATH=.env.test NODE_ENV=test vitest run",
    "test:services": "DB_PORT=5433 DB_NAME=cutabove_test DB_USER=postgres DB_PASSWORD=postgres docker-compose up -d db redis && pnpm test:db:create",
    "test:db:create": "node ./scripts/create-test-db.js",
    "test:services:down": "docker-compose down",
    "typecheck": "tsc --noEmit",
    "build:ui": "rm -rf dist && cd ../client && pnpm run build && mv dist ../server",
    "db:seed:dev": "NODE_ENV=development tsx ./src/utils/seeder.ts",
    "db:reset:dev": "NODE_ENV=development tsx ./src/utils/dbReset.ts",
    "docker:dev": "docker-compose up -d db redis",
    "docker:down": "docker-compose down",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
